<?xml version="1.0" encoding="utf-8"?>

<openerp>
<data>

<record id="sale_margin_report_liderit_graph" model="ir.ui.view">
    <field name="name">sale.margin.report.liderit.graph</field>
    <field name="model">sale.report</field>
    <field name="inherit_id" ref="sale.view_order_product_graph"/>
    <field name="arch" type="xml">
        <xpath expr="//field[@name='price_total']" position="after">
            <field name="margin_total" type="measure"/>
        </xpath>
    </field>
</record>


</data>
</openerp>
