<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">   
<t t-extend="ListView">

    <t t-jquery="th.oe_list_record_delete" t-operation="replace">
        <th t-if="options.deletable" class="oe_list_record_delete" width="13px"><span class="acc_filling_opt">Borrar elemento</span></th>
    </t>
</t>




<t t-extend="WebClient.timezone_systray">
    <t t-jquery="li.oe_topbar_item" t-operation="replace">
        <li class="oe_topbar_item oe_timezone_systray" title="Timezone mismatch">
            <a href="#"><em class="acc_filling_opt" style="font-style: normal;">Zona horaria incompatible</em><i class="fa fa-exclamation-triangle" style="color:red;"></i></a>
        </li>
    </t>
</t>

<t t-extend="im_chat.ImTopButton">
    <t t-jquery="a" t-operation="replace">
        <a href="#"><span class="acc_filling_opt">Chat</span><i class="fa fa-exclamation-triangle" style="color:red;"></i></a>
    </t>
</t>

<t t-extend="SearchView.SaveFilter">
    <t t-jquery="form.oe_form p.oe_form_required input" t-operation="before">
        <label for="oe_searchview_custom_input"><span class="acc_filling_opt">Buscar</span></label>
    </t>
</t>

<t t-extend="SearchView.addtodashboard">
    <t t-jquery="form p input" t-operation="replace">
        <label for="acc_dash_title"><span class="acc_filling_opt">Etiqueta sin texto</span></label>
        <input id="acc_dash_title" placeholder="Title of new dashboard item"/>
    </t>
</t>

<t t-extend="im_chat.InstantMessaging">
    <t t-jquery="div.oe_im_frame_header input" t-operation="replace">
        <label for="acc_oe_im_searchbox"><span class="acc_filling_opt">Buscar usuarios</span></label>
        <input id="acc_oe_im_searchbox" class="oe_im_searchbox" placeholder="Search users..."/>
    </t>
</t>

<t t-extend="SearchView.extended_search.proposition.char">
    <t t-jquery="input" t-operation="replace">
        <label for="acc_field_char"><span class="acc_filling_opt">Etiqueta sin texto</span></label>
        <input id="acc_field_char" class="field_char"/> 
     </t>
</t>

<t t-extend="ViewManager">
    <t t-jquery="td h2.oe_view_title span" t-operation="after">
        <span class="acc_filling_opt">cabecera de segundo nivel vacía</span>
     </t>
</t>

<t t-extend="mail.followers">
    <t t-jquery="span.caret" t-operation="after">
        <span class="acc_filling_opt">botón sin texto</span>
     </t>
</t>

<t t-extend="FieldBinaryImage">
    <t t-jquery="div.oe_form_binary_progress img" t-operation="replace">
        <img t-att-src='_s + "/web/static/src/img/throbber.gif"' alt="cargando..." width="16" height="16"/>
     </t>
</t>

<t t-extend="FieldMany2One">
    <t t-jquery="div span.oe_m2o_drop_down_button" t-operation="replace">
        <span class="oe_m2o_drop_down_button">
            <img t-att-src='_s + "/web/static/src/img/down-arrow.png"' alt="mostrar sugerenciasa" draggable="false"/>
        </span>
     </t>
     
</t>

<t t-extend="HiddenInputFile">
    <t t-jquery="form.oe_form_binary_form" t-operation="replace">
        <form class="oe_form_binary_form" t-att-target="fileupload_id"
            method="post" enctype="multipart/form-data" t-att-action="fileupload_action || '/web/binary/upload'">
            <input type="hidden" name="session_id" value="" t-if="widget.session.override_session"/>
            <input type="hidden" name="callback" t-att-value="fileupload_id"/>
            <t t-raw="0"/>
            <label for="acc_oe_form_binary_file" t-if="widget.widget!='image'"><span class="acc_filling_opt">ubida de archivo</span></label>
            <input type="file" id="acc_oe_form_binary_file" class="oe_form_binary_file" name="ufile" t-if="widget.widget!='image'"/>
            <label for="acc_oe_form_binary_file2" t-if="widget.widget=='image'"><span class="acc_filling_opt">Subida de archivo</span></label>
            <input type="file" id="acc_oe_form_binary_file2" class="oe_form_binary_file" name="ufile" accept="image/*" t-if="widget.widget=='image'"/>
        </form>
     </t>
</t>

<t t-extend="FieldMany2ManyTags">
    <t t-jquery="textarea" t-operation="replace">
        <textarea t-att-id="widget.id_for_label"
            rows="1" style="width: 100%"
            t-att-placeholder="widget.node.attrs.placeholder"></textarea>
    </t>
</t>

<t t-extend="FieldChar">
    <t t-jquery="input" t-operation="replace">
            <input t-att-type="widget.password ? 'password' : 'text'"
                t-att-id="widget.id_for_label"
                t-att-tabindex="widget.node.attrs.tabindex"
                t-att-autofocus="widget.node.attrs.autofocus"
                t-att-placeholder="widget.node.attrs.placeholder"
                t-att-maxlength="widget.field.size"
            />
     </t>
</t>

<t t-extend="web.datepicker">
    <t t-jquery="span" t-operation="replace">
        <span>
            <t t-set="placeholder" t-value="widget.getParent().node and widget.getParent().node.attrs.placeholder"/>
            <input type="text" class="oe_datepicker_container" disabled="disabled" style="display: none;"/>
            <input type="text"
                t-att-id="widget.id_for_label"
                t-att-name="widget.name"
                t-att-placeholder="placeholder"
                class="oe_datepicker_master"
            /><img alt="mostrar calendario" class="oe_input_icon oe_datepicker_trigger" draggable="false"
               t-att-src='_s + "/web/static/src/img/ui/field_calendar.png"'
               title="Select date" width="16" height="16" border="0"/>
        </span>
     </t>
</t>

<!-- WEB SHORTCUTS -->
<t t-extend="Systray.Shortcuts">
    <t t-jquery="li.dropdown" t-operation="replace">
        <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                <span class="acc_filling_opt">Mostrar atajos</span>
                <img alt="Añadir atajo" src="/web_shortcuts/static/src/img/add-shortcut.png"/>
            </a>
            <ul class="oe_systray_shortcuts_items dropdown-menu">
            </ul>
        </li>
     </t>
</t>

<t t-extend="ViewManagerAction">
    <t t-jquery="a.oe_shortcuts_toggle" t-operation="replace">
        <a t-if="widget.flags.display_title !== false" class="oe_shortcuts_toggle" title="Add / Remove Shortcut..."
            href="javascript: void(0)"><span class="acc_filling acc_small">Añadir / Eliminar atajo</span> </a>
    </t>
</t>

<t t-extend="mail.followers.partner">
    <t t-jquery="img" t-operation="replace">
        <img alt="Imagen de seguidor" class="oe_mail_thumbnail oe_mail_frame" t-att-src="record.avatar_url"/>
    </t>
</t>

<t t-extend="mail.thread.message">
    <t t-jquery="div.oe_msg_left" t-operation="replace">
        <div class='oe_msg_left'>
            <a t-if="widget.options.show_link" t-attf-href="#model=res.partner&amp;id=#{widget.author_id[0]}" t-att-title="widget.author_id[1]">
                <img alt="Icono de mensaje" class="oe_msg_icon" t-att-src="widget.avatar"/>
            </a>
            <img alt="Icono de mensaje" t-if="!widget.options.show_link" class="oe_msg_icon" t-att-src="widget.avatar"/>
        </div>
    </t>
</t>

            

</templates>



