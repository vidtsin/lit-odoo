<openerp>
    <data>

        <record model="ir.ui.view" id="mrp_laborales_form_view">
            <field name="name">mrp.laborales.inh.form</field>
            <field name="model">mrp.production</field>
            <field name="inherit_id" ref="mrp.mrp_production_form_view"/>
            <field name="arch" type="xml">
                <field name="date_start" position="replace">
                     <field name="date_start" readonly="1"/>
                </field>
                <field name="date_finished" position="replace">
                     <field name="date_finished" readonly="1"/>
                </field>
                <field name="date_finished" position="after">
                     <field name="num_dias_laborales"/>
                </field>
            </field>
        </record>


    </data>
</openerp>
